<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/root"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:fitsSystemWindows="true"
    style="@style/ScreenBackground"
    tools:context=".ui.main.MainActivity">

    <!-- Toolbar fixed di atas -->
    <com.google.android.material.appbar.MaterialToolbar
        android:id="@+id/toolbar"
        android:layout_width="0dp"
        android:layout_height="?attr/actionBarSize"
        android:background="@color/deep_blue"
        android:theme="@style/ThemeOverlay.AppCompat.Dark.ActionBar"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:popupTheme="@style/ThemeOverlay.AppCompat.Light"
        app:titleTextColor="@color/primary_cyan">

        <ImageView
            android:id="@+id/imgLogo"
            android:layout_width="28dp"
            android:layout_height="28dp"
            android:layout_gravity="start|center_vertical"
            android:layout_marginStart="12dp"
            android:contentDescription="Logo"
            android:src="@drawable/ic_logo_kodamjaya" />

        <TextView
            android:id="@+id/tvTitle"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:ellipsize="end"
            android:padding="4dp"
            android:singleLine="true"
            android:text=""
            android:textColor="@color/primary_cyan"
            android:textStyle="bold" />

        <ImageView
            android:id="@+id/btnMenu"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="end|center_vertical"
            android:background="?attr/selectableItemBackgroundBorderless"
            android:clickable="true"
            android:contentDescription="Menu"
            android:focusable="true"
            android:padding="12dp"
            android:src="@drawable/ic_menu_more" />
    </com.google.android.material.appbar.MaterialToolbar>

    <!-- Konten bisa discroll -->
    <androidx.core.widget.NestedScrollView
        android:id="@+id/scrollContent"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:fillViewport="true"
        android:clipToPadding="false"
        app:layout_constraintTop_toBottomOf="@id/toolbar"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <!-- ====== CARD PERSONEL ====== -->
            <com.google.android.material.card.MaterialCardView
                android:id="@+id/cardPersonel"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginHorizontal="16dp"
                android:layout_marginTop="8dp"
                app:cardBackgroundColor="@color/deep_blue_900"
                app:cardCornerRadius="14dp"
                app:strokeColor="@color/deep_blue_700"
                app:strokeWidth="1dp">

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:id="@+id/cardContent"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:padding="12dp"
                    style="@style/CardBackground">

                    <!-- ===== TITLE BOX ===== -->
                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:id="@+id/titleBox"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:paddingHorizontal="18dp"
                        android:paddingTop="8dp"
                        android:paddingBottom="8dp"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintTop_toTopOf="parent">

                        <TextView
                            android:id="@+id/tvDataPersonel"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:paddingVertical="6dp"
                            android:text="Data Personel"
                            android:textColor="@color/white"
                            android:textStyle="bold"
                            app:autoSizeMaxTextSize="16sp"
                            app:autoSizeMinTextSize="12sp"
                            app:autoSizeStepGranularity="1sp"
                            app:autoSizeTextType="uniform"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintTop_toTopOf="parent"
                            app:layout_constraintBottom_toBottomOf="parent"
                            tools:ignore="HardcodedText" />

                        <!-- Letter L - 4 sisi -->
                        <View
                            android:id="@+id/corner_tl_h"
                            android:layout_width="14dp"
                            android:layout_height="3dp"
                            android:background="@android:color/white"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent"/>
                        <View
                            android:id="@+id/corner_tl_v"
                            android:layout_width="3dp"
                            android:layout_height="14dp"
                            android:background="@android:color/white"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent"/>

                        <View
                            android:id="@+id/corner_tr_h"
                            android:layout_width="14dp"
                            android:layout_height="3dp"
                            android:background="@android:color/white"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintTop_toTopOf="parent"/>
                        <View
                            android:id="@+id/corner_tr_v"
                            android:layout_width="3dp"
                            android:layout_height="14dp"
                            android:background="@android:color/white"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintTop_toTopOf="parent"/>

                        <View
                            android:id="@+id/corner_bl_h"
                            android:layout_width="14dp"
                            android:layout_height="3dp"
                            android:background="@android:color/white"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintBottom_toBottomOf="parent"/>
                        <View
                            android:id="@+id/corner_bl_v"
                            android:layout_width="3dp"
                            android:layout_height="14dp"
                            android:background="@android:color/white"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintBottom_toBottomOf="parent"/>

                        <View
                            android:id="@+id/corner_br_h"
                            android:layout_width="14dp"
                            android:layout_height="3dp"
                            android:background="@android:color/white"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintBottom_toBottomOf="parent"/>
                        <View
                            android:id="@+id/corner_br_v"
                            android:layout_width="3dp"
                            android:layout_height="14dp"
                            android:background="@android:color/white"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintBottom_toBottomOf="parent"/>

                        <!-- Garis halus -->
                        <View
                            android:id="@+id/title_top_line"
                            android:layout_width="0dp"
                            android:layout_height="1dp"
                            android:background="@android:color/white"
                            android:layout_marginStart="18dp"
                            android:layout_marginEnd="18dp"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintTop_toTopOf="parent" />
                        <View
                            android:id="@+id/title_bottom_line"
                            android:layout_width="0dp"
                            android:layout_height="1dp"
                            android:background="@android:color/white"
                            android:layout_marginStart="18dp"
                            android:layout_marginEnd="18dp"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintBottom_toBottomOf="parent" />

                        <View
                            android:id="@+id/title_left_line"
                            android:layout_width="1dp"
                            android:layout_height="6dp"
                            android:layout_marginTop="18dp"
                            android:layout_marginBottom="18dp"
                            android:background="@android:color/white"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent" />

                        <View
                            android:id="@+id/title_right_line"
                            android:layout_width="1dp"
                            android:layout_height="6dp"
                            android:layout_marginTop="18dp"
                            android:layout_marginBottom="18dp"
                            android:background="@android:color/white"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintTop_toTopOf="parent" />
                    </androidx.constraintlayout.widget.ConstraintLayout>
                    <!-- ===== END TITLE BOX ===== -->

                    <!-- ===== INFO GRID ===== -->
                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:id="@+id/personRow"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:paddingTop="8dp"
                        app:layout_constraintTop_toBottomOf="@id/titleBox"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintEnd_toEndOf="parent">

                        <ImageView
                            android:id="@+id/imgPerson"
                            android:layout_width="98dp"
                            android:layout_height="98dp"
                            android:background="@drawable/bg_avatar"
                            android:contentDescription="Avatar"
                            android:scaleType="centerCrop"
                            android:src="@drawable/ic_soldier"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent" />

                        <androidx.constraintlayout.widget.ConstraintLayout
                            android:id="@+id/infoGrid"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="10dp"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toEndOf="@id/imgPerson"
                            app:layout_constraintTop_toTopOf="parent">

                            <androidx.constraintlayout.widget.Guideline
                                android:id="@+id/colGuide"
                                android:layout_width="0dp"
                                android:layout_height="0dp"
                                android:orientation="vertical"
                                app:layout_constraintGuide_percent="0.36" />

                            <TextView
                                android:id="@+id/tvNrpLabel"
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:includeFontPadding="false"
                                android:lineSpacingMultiplier="0.95"
                                android:text="NRP"
                                android:textColor="@color/white"
                                android:textSize="12sp"
                                app:layout_constraintStart_toStartOf="parent"
                                app:layout_constraintEnd_toStartOf="@id/colGuide"
                                app:layout_constraintTop_toTopOf="parent"
                                tools:ignore="HardcodedText" />

                            <TextView
                                android:id="@+id/tvNrpPersonel"
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:includeFontPadding="false"
                                android:lineSpacingMultiplier="0.95"
                                android:maxLines="1"
                                android:text="--"
                                android:textColor="@color/white"
                                android:textSize="12sp"
                                app:layout_constraintStart_toStartOf="@id/colGuide"
                                app:layout_constraintEnd_toEndOf="parent"
                                app:layout_constraintTop_toTopOf="@id/tvNrpLabel"
                                tools:ignore="HardcodedText" />

                            <TextView
                                android:id="@+id/tvNameLabel"
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="4dp"
                                android:includeFontPadding="false"
                                android:lineSpacingMultiplier="0.95"
                                android:text="Name"
                                android:textColor="@color/white"
                                android:textSize="12sp"
                                app:layout_constraintEnd_toStartOf="@id/colGuide"
                                app:layout_constraintStart_toStartOf="parent"
                                app:layout_constraintTop_toBottomOf="@id/tvNrpLabel"
                                tools:ignore="HardcodedText" />

                            <TextView
                                android:id="@+id/tvNamePersonel"
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:ellipsize="end"
                                android:includeFontPadding="false"
                                android:lineSpacingMultiplier="0.95"
                                android:maxLines="1"
                                android:text="--"
                                android:textColor="@color/white"
                                android:textSize="12sp"
                                app:layout_constraintEnd_toEndOf="parent"
                                app:layout_constraintStart_toStartOf="@id/colGuide"
                                app:layout_constraintTop_toTopOf="@id/tvNameLabel"
                                tools:ignore="HardcodedText" />

                            <TextView
                                android:id="@+id/tvRankLabel"
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="4dp"
                                android:includeFontPadding="false"
                                android:lineSpacingMultiplier="0.95"
                                android:text="Rank"
                                android:textColor="@color/white"
                                android:textSize="12sp"
                                app:layout_constraintEnd_toStartOf="@id/colGuide"
                                app:layout_constraintStart_toStartOf="parent"
                                app:layout_constraintTop_toBottomOf="@id/tvNameLabel"
                                tools:ignore="HardcodedText" />

                            <TextView
                                android:id="@+id/tvRankPersonel"
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:ellipsize="end"
                                android:includeFontPadding="false"
                                android:lineSpacingMultiplier="0.95"
                                android:maxLines="1"
                                android:text="--"
                                android:textColor="@color/white"
                                android:textSize="12sp"
                                app:layout_constraintEnd_toEndOf="parent"
                                app:layout_constraintStart_toStartOf="@id/colGuide"
                                app:layout_constraintTop_toTopOf="@id/tvRankLabel"
                                tools:ignore="HardcodedText" />

                            <TextView
                                android:id="@+id/tvUnitLabel"
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="4dp"
                                android:includeFontPadding="false"
                                android:lineSpacingMultiplier="0.95"
                                android:text="Unit"
                                android:textColor="@color/white"
                                android:textSize="12sp"
                                app:layout_constraintEnd_toStartOf="@id/colGuide"
                                app:layout_constraintStart_toStartOf="parent"
                                app:layout_constraintTop_toBottomOf="@id/tvRankLabel"
                                tools:ignore="HardcodedText" />

                            <TextView
                                android:id="@+id/tvUnitPersonel"
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:ellipsize="end"
                                android:includeFontPadding="false"
                                android:lineSpacingMultiplier="0.95"
                                android:maxLines="1"
                                android:text="--"
                                android:textColor="@color/white"
                                android:textSize="12sp"
                                app:layout_constraintEnd_toEndOf="parent"
                                app:layout_constraintStart_toStartOf="@id/colGuide"
                                app:layout_constraintTop_toTopOf="@id/tvUnitLabel"
                                tools:ignore="HardcodedText" />

                            <TextView
                                android:id="@+id/tvBattalionLabel"
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="4dp"
                                android:includeFontPadding="false"
                                android:lineSpacingMultiplier="0.95"
                                android:text="Battalion"
                                android:textColor="@color/white"
                                android:textSize="12sp"
                                app:layout_constraintEnd_toStartOf="@id/colGuide"
                                app:layout_constraintStart_toStartOf="parent"
                                app:layout_constraintTop_toBottomOf="@id/tvUnitLabel"
                                tools:ignore="HardcodedText" />

                            <TextView
                                android:id="@+id/tvBattalionPersonel"
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:ellipsize="end"
                                android:includeFontPadding="false"
                                android:lineSpacingMultiplier="0.95"
                                android:maxLines="1"
                                android:text="--"
                                android:textColor="@color/white"
                                android:textSize="12sp"
                                app:layout_constraintEnd_toEndOf="parent"
                                app:layout_constraintStart_toStartOf="@id/colGuide"
                                app:layout_constraintTop_toTopOf="@id/tvBattalionLabel"
                                tools:ignore="HardcodedText" />

                            <TextView
                                android:id="@+id/tvSquadLabel"
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="4dp"
                                android:includeFontPadding="false"
                                android:lineSpacingMultiplier="0.95"
                                android:text="Squad"
                                android:textColor="@color/white"
                                android:textSize="12sp"
                                app:layout_constraintEnd_toStartOf="@id/colGuide"
                                app:layout_constraintStart_toStartOf="parent"
                                app:layout_constraintTop_toBottomOf="@id/tvBattalionLabel"
                                tools:ignore="HardcodedText" />

                            <TextView
                                android:id="@+id/tvSquadPersonel"
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:ellipsize="end"
                                android:includeFontPadding="false"
                                android:lineSpacingMultiplier="0.95"
                                android:maxLines="1"
                                android:text="--"
                                android:textColor="@color/white"
                                android:textSize="12sp"
                                app:layout_constraintEnd_toEndOf="parent"
                                app:layout_constraintStart_toStartOf="@id/colGuide"
                                app:layout_constraintTop_toTopOf="@id/tvSquadLabel"
                                tools:ignore="HardcodedText" />
                        </androidx.constraintlayout.widget.ConstraintLayout>
                    </androidx.constraintlayout.widget.ConstraintLayout>
                </androidx.constraintlayout.widget.ConstraintLayout>
            </com.google.android.material.card.MaterialCardView>
            <!-- ====== END CARD PERSONEL ====== -->

            <!-- ====== MAP SECTION (tinggi adaptif via dimen) ====== -->
            <FrameLayout
                android:id="@+id/mapContainer"
                android:layout_width="match_parent"
                android:layout_height="@dimen/map_height"
                android:layout_marginHorizontal="16dp"
                android:layout_marginTop="8dp"
                android:layout_marginBottom="16dp"
                android:minHeight="160dp"
                tools:layout_height="320dp">

                <org.osmdroid.views.MapView
                    android:id="@+id/mapView"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent" />

                <com.google.android.material.card.MaterialCardView
                    android:id="@+id/heartCard"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="top|start"
                    android:layout_margin="16dp"
                    app:cardBackgroundColor="@color/deep_blue_900"
                    app:cardCornerRadius="16dp"
                    app:strokeColor="@color/deep_blue_700"
                    app:strokeWidth="1dp">
                    <TextView
                        android:id="@+id/tvHeartPersonel"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:includeFontPadding="false"
                        android:paddingStart="12dp"
                        android:paddingTop="6dp"
                        android:paddingEnd="12dp"
                        android:paddingBottom="6dp"
                        android:text="-- bpm"
                        android:textColor="@color/white"
                        android:textSize="12sp" />
                </com.google.android.material.card.MaterialCardView>

                <com.google.android.material.card.MaterialCardView
                    android:id="@+id/intervalCard"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="top|start"
                    android:layout_marginStart="16dp"
                    android:layout_marginTop="48dp"
                    app:cardBackgroundColor="@color/deep_blue_900"
                    app:cardCornerRadius="16dp"
                    app:strokeColor="@color/deep_blue_700"
                    app:strokeWidth="1dp">

                    <TextView
                        android:id="@+id/tvInterval"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:includeFontPadding="false"
                        android:paddingStart="12dp"
                        android:paddingTop="6dp"
                        android:paddingEnd="12dp"
                        android:paddingBottom="6dp"
                        android:text="Interval: --"
                        android:textColor="@color/white"
                        android:textSize="12sp" />
                </com.google.android.material.card.MaterialCardView>

                <com.google.android.material.card.MaterialCardView
                    android:id="@+id/batteryCard"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="top|end"
                    android:layout_margin="16dp"
                    app:cardBackgroundColor="@color/deep_blue_900"
                    app:cardCornerRadius="16dp"
                    app:strokeColor="@color/deep_blue_700"
                    app:strokeWidth="1dp">
                    <LinearLayout
                        android:id="@+id/batteryValueRow"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:paddingStart="12dp"
                        android:paddingTop="6dp"
                        android:paddingEnd="12dp"
                        android:paddingBottom="6dp">
                        <ImageView
                            android:id="@+id/imgBattery"
                            android:layout_width="14dp"
                            android:layout_height="14dp"
                            android:layout_marginEnd="2dp"
                            android:contentDescription="Battery icon"
                            android:src="@drawable/ic_battery_full"
                            app:tint="@android:color/holo_green_light" />
                        <TextView
                            android:id="@+id/tvBatteryPersonel"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:includeFontPadding="false"
                            android:lineSpacingMultiplier="0.95"
                            android:text="-- %"
                            android:textColor="@color/white"
                            android:textSize="12sp"
                            tools:text="90%" />
                    </LinearLayout>
                </com.google.android.material.card.MaterialCardView>

                <!-- FLOATING FABs: Zoom In, Zoom Out, My Location -->
                <LinearLayout
                    android:id="@+id/mapFabs"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical|end"
                    android:orientation="vertical"
                    android:layout_marginEnd="16dp"
                    android:layout_marginTop="0dp"
                    android:layout_marginBottom="0dp">

                    <com.google.android.material.floatingactionbutton.FloatingActionButton
                        android:id="@+id/fabZoomIn"
                        style="@style/App.SmallFab"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:contentDescription="Zoom In"
                        app:srcCompat="@drawable/ic_zoom_in"
                        app:backgroundTint="@color/deep_blue"
                        app:tint="@android:color/white" />

                    <com.google.android.material.floatingactionbutton.FloatingActionButton
                        android:id="@+id/fabZoomOut"
                        style="@style/App.SmallFab"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="6dp"
                        android:contentDescription="Zoom Out"
                        app:srcCompat="@drawable/ic_zoom_out"
                        app:backgroundTint="@color/deep_blue"
                        app:tint="@android:color/white" />

                    <com.google.android.material.floatingactionbutton.FloatingActionButton
                        android:id="@+id/fabMyLocation"
                        style="@style/App.SmallFab"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="6dp"
                        android:contentDescription="My Location"
                        app:srcCompat="@drawable/ic_my_location"
                        app:backgroundTint="@color/deep_blue"
                        app:tint="@android:color/white" />
                </LinearLayout>

                <com.google.android.material.card.MaterialCardView
                    android:id="@+id/coordCard"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="bottom|center_horizontal"
                    android:layout_marginBottom="16dp"
                    app:cardBackgroundColor="@color/deep_blue_900"
                    app:cardCornerRadius="16dp"
                    app:strokeColor="@color/deep_blue_700"
                    app:strokeWidth="1dp">
                    <TextView
                        android:id="@+id/tvCoordinates"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:includeFontPadding="false"
                        android:paddingStart="12dp"
                        android:paddingTop="6dp"
                        android:paddingEnd="12dp"
                        android:paddingBottom="6dp"
                        android:textColor="@color/white"
                        style="@style/TextColor"
                        android:textSize="12sp"
                        android:typeface="monospace" />
                </com.google.android.material.card.MaterialCardView>
            </FrameLayout>
            <!-- ====== END MAP SECTION ====== -->

        </LinearLayout>
    </androidx.core.widget.NestedScrollView>

</androidx.constraintlayout.widget.ConstraintLayout>
